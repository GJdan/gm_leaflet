<?php
/**
 * @file gm_test_maps.module
 */

/**
 * Implements hook_ctools_plugin_directory().
 */
function gm_test_maps_ctools_plugin_directory($module, $plugin) {
  return 'plugins/' . $plugin;
}

/**
 * Implements hook_menu().
 */
function gm_test_maps_menu() {
  $items = array();

  $items['gm_test_maps'] = array(
    'title' => 'TEST MAPS',
    'page callback' => 'gm_test_maps_test_maps',
    'access callback' => TRUE,
    'type' => MENU_CALLBACK,
  );

  return $items;
}

function gm_test_maps_test_maps() {
  $output = gm_leaflet_render_map('example_map', 'gm_test_maps_1');

  return $output;
}
